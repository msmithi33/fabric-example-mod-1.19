{
  "schemaVersion": 1,
  "id": "tutmod",
  "version": "${version}",

  "name": "Tutorial Mod",
  "description": "This is an example description! Tell everyone what your mod is about!",
  "authors": [
    "smithit!"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "MIT",
  "icon": "assets/tutmod/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "io.smithit.example.TutMod"
    ]
  },
  "mixins": [
    "tutmod.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.9",
    "fabric-api": "*",
    "minecraft": "~1.19",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
